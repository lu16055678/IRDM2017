                     <h3><a href="prospective_students/mres_csml/#jfmulticontent_c29660-3" rel="">COMPGM98 MRes Dissertation</a></h3> 
                     <div> 
                      <!--  CONTENT ELEMENT, uid:26053/text [begin] --> 
                      <div id="c26053" class="csc-default"> 
                       <!--  Header: [begin] --> 
                       <div class="csc-header csc-header-n1">
                        <h1 class="csc-firstHeader">COMPGM98 MRes Dissertation</h1>
                       </div> 
                       <!--  Header: [end] --> 
                       <!--  Text: [begin] --> 
                       <p class="align-justify">A research project over a period of 9 months.</p> 
                       <!--  Text: [end] --> 
