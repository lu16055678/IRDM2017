jQuery(document).ready(function($) {
	$('#jfmulticontent_c25548 > ul > li a').each(function() {
		var href = $(this).attr('href');
		var hrefProtocol = /^(?:(ht|f)tp(s?)\:\/\/)?/.exec(href);
		if (hrefProtocol[0] == "") {
			var temp = href.split("#");
			var host = window.location.href.split("#");
			$(this).attr('href', host[0] + "#" + temp[1]);
		}
	});
	$('#jfmulticontent_c25548').tabs({
		collapsible:true, heightStyle:'content', activate:function(e,ui) { jQuery.cookie('jfmulticontent_c25548', ui.newTab.index(), { expires: 30, path:'/prospective_students/msc_cs/' }); }, active:jQuery.cookie('jfmulticontent_c25548'), hide:{effect:'fadeOut'}, show:{effect:'fadeIn'}
	});
	
	
});

