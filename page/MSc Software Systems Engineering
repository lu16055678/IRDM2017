	$('#jfmulticontent_c30031 > ul > li a').each(function() {
		var href = $(this).attr('href');
		var hrefProtocol = /^(?:(ht|f)tp(s?)\:\/\/)?/.exec(href);
		if (hrefProtocol[0] == "") {
			var temp = href.split("#");
			var host = window.location.href.split("#");
			$(this).attr('href', host[0] + "#" + temp[1]);
		}
	});
	$('#jfmulticontent_c30031').tabs({
		collapsible:true, heightStyle:'content', activate:function(e,ui) { jQuery.cookie('jfmulticontent_c30031', ui.newTab.index(), { expires: 30, path:'/prospective_students/msc_sse/' }); }, active:jQuery.cookie('jfmulticontent_c30031'), hide:{effect:'fadeOut'}, show:{effect:'fadeIn'}
	});
	
	
});

